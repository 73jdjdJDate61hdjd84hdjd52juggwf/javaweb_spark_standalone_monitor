<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>WordCount</servlet-name>
        <servlet-class>demo.servlet.WordCountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WordCount</servlet-name>
        <url-pattern>/WordCount</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>WordCount2</servlet-name>
        <servlet-class>demo.servlet.WordCountServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WordCount2</servlet-name>
        <url-pattern>/WordCount2</url-pattern>
    </servlet-mapping>

</web-app>
